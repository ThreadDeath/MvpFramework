<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true">
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke android:width="1dp" android:color="@color/jmui_default_cursor_color" />
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FFFFFF" />
            <corners android:radius="2dp"/>
            <stroke android:width="1dp" android:color="@color/jmui_chat_detail_split_line_color" />
        </shape>
    </item>

</selector>